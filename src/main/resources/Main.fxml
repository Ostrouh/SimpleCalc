<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane"
            maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="300.0" prefWidth="200.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.112"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Controller">
   <children>
      <TextField fx:id="textField" layoutX="15.0" layoutY="35.0" prefHeight="25.0" prefWidth="175.0" />
      <Button fx:id="buttonC"  defaultButton="true" layoutX="15.0" layoutY="100.0" mnemonicParsing="false"
              onAction="#onClickButtonC" prefWidth="40.0" text="C" />
      <Button fx:id="button7" defaultButton="true" layoutX="15.0" layoutY="135.0" mnemonicParsing="false"
              onAction="#onClickButton7" prefWidth="30.0" text="7" />
      <Button fx:id="button8" defaultButton="true" layoutX="50.0" layoutY="135.0" mnemonicParsing="false"
              onAction="#onClickButton8" prefWidth="30.0" text="8" />
      <Button fx:id="button9" defaultButton="true" layoutX="85.0" layoutY="135.0" mnemonicParsing="false"
              onAction="#onClickButton9" prefWidth="30.0" text="9" />
      <Button fx:id="buttonMultiply" defaultButton="true" layoutX="150.0" layoutY="135.0" mnemonicParsing="false"
              onAction="#onClickButtonMultiply" prefWidth="40.0" text="*" />
      <Button fx:id="button4" defaultButton="true" layoutX="15.0" layoutY="170.0" mnemonicParsing="false"
              onAction="#onClickButton4" prefWidth="30.0" text="4" />
      <Button fx:id="button5" defaultButton="true" layoutX="50.0" layoutY="170.0" mnemonicParsing="false"
              onAction="#onClickButton5" prefWidth="30.0" text="5" />
      <Button fx:id="button6" defaultButton="true" layoutX="85.0" layoutY="170.0" mnemonicParsing="false"
              onAction="#onClickButton6" prefWidth="30.0" text="6" />
      <Button fx:id="buttonDivide" defaultButton="true" layoutX="150.0" layoutY="170.0" mnemonicParsing="false"
              onAction="#onClickButtonDivide" prefWidth="40.0" text="/" />
      <Button fx:id="button1" defaultButton="true" layoutX="15.0" layoutY="205.0" mnemonicParsing="false"
              onAction="#onClickButton1" prefWidth="30.0" text="1" />
      <Button fx:id="button2" defaultButton="true" layoutX="50.0" layoutY="205.0" mnemonicParsing="false"
              onAction="#onClickButton2" prefWidth="30.0" text="2" />
      <Button fx:id="button3" defaultButton="true" layoutX="85.0" layoutY="205.0" mnemonicParsing="false"
              onAction="#onClickButton3" prefWidth="30.0" text="3" />
      <Button fx:id="buttonPlus" defaultButton="true" layoutX="150.0" layoutY="205.0" mnemonicParsing="false"
              onAction="#onClickButtonPlus" prefWidth="40.0" text="+" />
      <Button fx:id="button0" defaultButton="true" layoutX="50.0" layoutY="240.0" mnemonicParsing="false"
              onAction="#onClickButton0" prefWidth="30.0" text="0" />
      <Button fx:id="buttonDot" defaultButton="true" layoutX="15.0" layoutY="240.0" mnemonicParsing="false"
              onAction="#onClickButtonDot" prefWidth="30.0" text="." />
      <Button fx:id="buttonMinus" defaultButton="true" layoutX="150.0" layoutY="240.0" mnemonicParsing="false"
              onAction="#onClickButtonMinus" prefWidth="40.0" text="-" />
      <Button fx:id="buttonEqual" defaultButton="true" layoutX="85.0" layoutY="240.0" mnemonicParsing="false"
              onAction="#onClickButtonEqual" prefHeight="30.0" prefWidth="50.0" text="=" />
      <Button fx:id="buttonToDegrees" defaultButton="true" layoutX="150.0" layoutY="100.0" mnemonicParsing="false"
              onAction="#onClickButtonToDegrees" prefWidth="40.0" text="^" />
      <Button fx:id="buttonBS" defaultButton="true" layoutX="75.0" layoutY="100.0" mnemonicParsing="false"
              onAction="#onClickButtonBS" prefWidth="40.0" text="BS" />
   </children>
</AnchorPane>
