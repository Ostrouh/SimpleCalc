<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="300.0" prefWidth="180.0"
            xmlns="http://javafx.com/javafx/8.0.112"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.Controller">
   <children>
      <TextField fx:id="tf" layoutX="15.0" layoutY="30.0" prefHeight="25.0" prefWidth="150.0" />
      <Button fx:id="buttonC" layoutX="15.0" layoutY="100.0" mnemonicParsing="false" onAction="#onClickButtonC" prefWidth="30.0" text="C" />
      <Button fx:id="button7" layoutX="15.0" layoutY="135.0" mnemonicParsing="false" onAction="#onClickButton7" prefWidth="30.0" text="7" />
      <Button fx:id="button8" layoutX="50.0" layoutY="135.0" mnemonicParsing="false" onAction="#onClickButton8" prefWidth="30.0" text="8" />
      <Button fx:id="button9" layoutX="85.0" layoutY="135.0" mnemonicParsing="false" onAction="#onClickButton9" prefWidth="30.0" text="9" />
      <Button fx:id="buttonMultiply" layoutX="135.0" layoutY="135.0" mnemonicParsing="false" onAction="#onClickButtonMultiply" prefWidth="30.0" text="*" />
      <Button fx:id="button4" layoutX="15.0" layoutY="170.0" mnemonicParsing="false" onAction="#onClickButton4" prefWidth="30.0" text="4" />
      <Button fx:id="button5" layoutX="50.0" layoutY="170.0" mnemonicParsing="false" onAction="#onClickButton5" prefWidth="30.0" text="5" />
      <Button fx:id="button6" layoutX="85.0" layoutY="170.0" mnemonicParsing="false" onAction="#onClickButton6" prefWidth="30.0" text="6" />
      <Button fx:id="buttonDivide" layoutX="135.0" layoutY="170.0" mnemonicParsing="false" onAction="#onClickButtonDivide" prefWidth="30.0" text="/" />
      <Button fx:id="button1" layoutX="15.0" layoutY="205.0" mnemonicParsing="false" onAction="#onClickButton1" prefWidth="30.0" text="1" />
      <Button fx:id="button2" layoutX="50.0" layoutY="205.0" mnemonicParsing="false" onAction="#onClickButton2" prefWidth="30.0" text="2" />
      <Button fx:id="button3" layoutX="85.0" layoutY="205.0" mnemonicParsing="false" onAction="#onClickButton3" prefWidth="30.0" text="3" />
      <Button fx:id="buttonPlus" layoutX="135.0" layoutY="205.0" mnemonicParsing="false" onAction="#onClickButtonPlus" prefWidth="30.0" text="+" />
      <Button fx:id="button0" layoutX="50.0" layoutY="240.0" mnemonicParsing="false" onAction="#onClickButton0" prefWidth="30.0" text="0" />
      <Button fx:id="buttonDot" layoutX="15.0" layoutY="240.0" mnemonicParsing="false" onAction="#onClickButtonDot" prefWidth="30.0" text="." />
      <Button fx:id="buttonMinus" layoutX="135.0" layoutY="240.0" mnemonicParsing="false" onAction="#onClickButtonMinus" prefWidth="30.0" text="-" />
      <Button fx:id="buttonEqual" layoutX="85.0" layoutY="240.0" mnemonicParsing="false" onAction="#onClickButtonEqual" prefHeight="25.0" prefWidth="45.0" text="=" />
      <Button fx:id="buttonToDegrees" layoutX="135.0" layoutY="100.0" mnemonicParsing="false" onAction="#onClickButtonToDegrees" prefWidth="30.0" text="^" />
      <Button fx:id="buttonBS" layoutX="75.0" layoutY="100.0" mnemonicParsing="false" onAction="#onClickButtonBS" text="BS" />
   </children>
</AnchorPane>
